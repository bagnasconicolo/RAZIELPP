{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/src",
                "/opt/homebrew/opt/qt@5/include",
                "/opt/homebrew/opt/qt@5/include/QtCore",
                "/opt/homebrew/opt/qt@5/include/QtWidgets",
                "/opt/homebrew/opt/qt@5/include/QtGui",
                "/opt/homebrew/opt/opencv/include",
                "/opt/homebrew/include",
                "/opt/homebrew/include/opencv4"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64",
            "browse": {
                "path": [
                    "${workspaceFolder}/include",
                    "${workspaceFolder}/src",
                    "/opt/homebrew/opt/qt@5/include",
                    "/opt/homebrew/opt/opencv/include",
                    "/opt/homebrew/include",
                    "/opt/homebrew/include/opencv4"
                ],
                "limitSymbolsToIncludedHeaders": false
            },
            "compilerArgs": [
                "-F/opt/homebrew/opt/qt@5/lib"
            ]
        }
    ],
    "version": 4
}